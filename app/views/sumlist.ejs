<!doctype html>
<html>
  <head>
    <meta charset = "utf-8">
    <title>Sum List</title>
    <link rel = "stylesheet" href = "/css/stylesheet-sumlist.css">
  </head>
  <body>
    <h1>Sum List</h1>
    <table>
      <% for(var i = 0; i < nums.length; i++){ %>
      <% var colors = (nums[i] % 2) ? odd : even %>
        <tr>
          <td style="color:<%= colors %>">
            <%= nums[i] %>
          </td>
        </tr>
      <% }; %>
        <td><%= sum %></td>
    </table>
  </body>
</html>
